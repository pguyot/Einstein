	MACRO
	NATIVE_PRIM	$id, $symbol
	AREA		$symbol, CODE, READONLY
	EXPORT		$symbol
	stmdb		sp!, {lr}
	ldr			lr, id_for_$symbol
	mcr			p10, 0, lr, c0, c0
	ldmia		sp!, {pc}
id_for_$symbol
	DCD			$id
	MEND

